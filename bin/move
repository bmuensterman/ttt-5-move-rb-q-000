#!/usr/bin/env ruby

require_relative '../lib/move.rb'


puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
input = gets.strip


board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
move(board, 0, "X")
display_board(board)
move(board, 4, "O")
display_board(board)
move(board, 8, "X")
display_board(board)
move(board, 0, "O")
display_board(board)
move(board, 8, "X")
display_board(board)

